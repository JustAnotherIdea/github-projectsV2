{
  "name": "vscode-github-projects-v2",
  "displayName": "GitHub Projects V2",
  "description": "View and manage GitHub Projects V2 boards",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "github-projects",
          "title": "GitHub Projects",
          "icon": "resources/project.svg"
        }
      ]
    },
    "views": {
      "github-projects": [
        {
          "id": "github-projects-boards",
          "name": "Boards",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "github-projects.openBoard",
        "title": "Open GitHub Projects Board"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "eslint": "^8.26.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "process": "^0.11.10",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
